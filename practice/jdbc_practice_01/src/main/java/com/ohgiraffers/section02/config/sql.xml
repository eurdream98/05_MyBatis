<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>
    <comment/>
    <entry key="selectAllMenu">
       SELECT
        *
        FROM
        tbl_menu
    </entry>

    <entry key="selectMenuMenuCode">
        SELECT
        *
        FROM
        tbl_menu
        WHERE
        menu_code=?
    </entry>

    <entry key="insert">
        INSERT INTO
        tbl_menu(menu_name,menu_price,category_code,orderable_status)
        VALUES
        (?,?,?,?)
    </entry>

    <entry key="update">
        UPDATE
        tbl_menu
        SET
        menu_name=?,
        menu_price=?,
        category_code=?,
        orderable_status=?
        WHERE
        menu_code=?
    </entry>

    <entry key="delete">
        DELETE
        FROM
        tbl_menu
        WHERE
        menu_code=?
    </entry>


</properties>